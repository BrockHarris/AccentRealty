Pages#home

<h3>Contact form:</h3>
<%= form_for @message, :html=> { :multipart => true} do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :subject %>
  <%= f.text_field :subject %>

  <%= f.label :body %>
  <%= f.text_area :body %>
   
  <%= f.submit "Send" %>
<% end %>