
<div class="carousel_container">

<div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
    <li data-target="#carousel-example-generic" data-slide-to="1"></li>
    <li data-target="#carousel-example-generic" data-slide-to="2"></li>
    <li data-target="#carousel-example-generic" data-slide-to="3"></li>
    <li data-target="#carousel-example-generic" data-slide-to="4"></li>
  </ol>

  <!-- Wrapper for slides -->
  <div class="carousel-inner">

    <div class="item active">
      <%= image_tag "house1.jpg" %>
      <div class="carousel-caption caption_mod">
        Ashford, CT<br/>
        $575,000<br/>
        3 Bedrooms | 2 Baths | 1.25 Acres
      </div>
    </div>

    <div class="item">
      <%= image_tag "house2.jpg" %>
      <div class="carousel-caption caption_mod">
        Caption
      </div>
    </div>

    <div class="item">
      <%= image_tag "house3.jpg" %>
      <div class="carousel-caption caption_mod">
        Caption
      </div>
    </div>

    <div class="item">
      <%= image_tag "house4.jpg" %>
      <div class="carousel-caption caption_mod">
        Caption
      </div>
    </div>
    
    <div class="item">
      <%= image_tag "house5.jpg" %>
      <div class="carousel-caption caption_mod">
        Caption
      </div>
    </div>
  </div>

  <!-- Controls -->
  <a class="left carousel-control" href="#carousel-example-generic" data-slide="prev">
    <span class="glyphicon glyphicon-chevron-left"></span>
  </a>
  <a class="right carousel-control" href="#carousel-example-generic" data-slide="next">
    <span class="glyphicon glyphicon-chevron-right"></span>
  </a>
</div>

</div>

<div class="home_side_col">
  <h3>Middle Section</h3>
  <hr>
  Item 1 
  <hr>
  Item 2
  <hr>
  Item 3
</div>