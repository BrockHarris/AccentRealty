<table class="table table-hover">
  <thead>
     <tr>
      <th>Username</th>
      <th>First</th>
      <th>Last</th>
      <th>Email</th>
     <tr>
  </thead>
  <tbody>
    <% for admin in @show_admins %>
    <tr>
      <td><%= admin.username %></td> 
      <td><%= admin.firstname %></td> 
      <td><%= admin.lastname %></td> 
      <td><%= admin.email %></td> 
      <td><%# link_to "Delete", user_path(user), :method=>:delete  %></td>
    <tr>
    <% end %>
  </tbody>
</table>